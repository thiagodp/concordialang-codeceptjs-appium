{
  "name": "concordialang-codeceptjs-appium",
  "version": "0.1.0",
  "description": "Concordia Language plug-in to generate test scripts for CodeceptJS with Appium",
  "license": "AGPL-3.0",
  "readme": "readme.md",
  "homepage": "https://github.com/thiagodp/concordialang-codeceptjs-appium#readme",
  "author": {
    "name": "Thiago Delgado Pinto",
    "email": "thiago-dp@bol.com.br"
  },
  "keywords": [
    "concordialang",
    "concordia-lang",
    "concordia",
    "codeceptjs",
    "appium",
    "test",
    "script",
    "e2e",
    "plugin",
    "plug-in"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "postversion": "git push && git push --tags"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagodp/concordialang-codeceptjs-appium.git"
  },
  "bugs": {
    "url": "https://github.com/thiagodp/concordialang-codeceptjs-appium/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "concordialang-codeceptjs-webdriverio": "^0.1.0"
  }
}
