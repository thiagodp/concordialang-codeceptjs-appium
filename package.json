{
  "name": "concordialang-codeceptjs-appium",
  "version": "1.3.2",
  "description": "Concordia Language plug-in to generate test scripts for CodeceptJS with Appium",
  "license": "AGPL-3.0",
  "readme": "readme.md",
  "homepage": "https://github.com/thiagodp/concordialang-codeceptjs-appium",
  "author": {
    "name": "Thiago Delgado Pinto",
    "email": "thiago-dp@bol.com.br"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagodp/concordialang-codeceptjs-appium.git"
  },
  "bugs": {
    "url": "https://github.com/thiagodp/concordialang-codeceptjs-appium/issues"
  },
  "keywords": [
    "concordialang",
    "concordia-lang",
    "concordia",
    "codeceptjs",
    "appium",
    "test",
    "script",
    "e2e",
    "plugin",
    "plug-in"
  ],
  "main": "dist",
  "types": "dist",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "switch:local": "npm uninstall concordialang-codeceptjs-core && npm i ../concordialang-codeceptjs-core",
    "switch:web": "npm uninstall concordialang-codeceptjs-core && npm i concordialang-codeceptjs-core",
    "build": "(rimraf dist || echo \"rimraf not installed\") && tsc",
    "test": "jest --passWithNoTests",
    "preversion": "npm run build && npm run test",
    "postversion": "git push && git push --tags",
    "postinstall": "((npm list -g npx) || (npm i --color=always -g npx)) && node dist/postinstall.js"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "concordiaPlugin": {
    "isFake": false,
    "targets": [
      "codeceptjs",
      "appium"
    ],
    "file": "dist/CodeceptJSAppium.js",
    "class": "CodeceptJSAppium",
    "serve": "npx appium"
  },
  "dependencies": {
    "appium": "^1.17.0",
    "codeceptjs": "=1.2.1",
    "codeceptjs-cmdhelper": "^2.2.0",
    "codeceptjs-dbhelper": "^1.0.0",
    "concordialang-codeceptjs-core": "^0.5.0",
    "concordialang-plugin": "^0.6.1",
    "database-js": "^3.0.9",
    "database-js-json": "^1.2.1",
    "mocha": "=5.2.0",
    "mocha-multi": "=1.0.1"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.46",
    "@types/jest": "^25.1.4",
    "@types/node": "^8.10.59",
    "jest": "^25.2.3",
    "rimraf": "^2.7.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
